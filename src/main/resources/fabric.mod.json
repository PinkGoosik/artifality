{
  "schemaVersion": 1,
  "id": "artifality",
  "version": "${version}",

  "name": "Artifality",
  "description": "Artifality is a Minecraft Mod that adds some new Items, Blocks, Enchantments, and Other Stuff to your Minecraft world.",
  "authors": [
    "PinkGoosik",
    "DoctorNight",
    "Spusik",
    "aSomik"
  ],
  "contributors": [
    "matthewperiut",
    "PrismaticYT",
    "VanillaTweaks team"
  ],
  "contact": {
    "homepage": "https://curseforge.com/minecraft/mc-mods/artifality",
    "sources": "https://github.com/PinkGoosik/artifality",
    "issues": "https://github.com/PinkGoosik/artifality/issues"
  },

  "license": "MIT",
  "icon": "assets/artifality/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": ["artifality.ArtifalityMod"],
    "client": ["artifality.client.ArtifalityClient"]
  },

  "mixins": [
    "artifality.mixins.json"
  ],

  "custom": {
    "modmenu": {
      "links": {
        "misc.artifality.discord": "https://discord.gg/DcemWeskeZ",
        "misc.artifality.wiki": "https://pinkgoosik.github.io/artifality"
      }
    }
  },

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.1",
    "java": ">=16",
    "trinkets": ">=3.0.4"
  },

  "suggests": {
    "bclib": "*"
  }
}
